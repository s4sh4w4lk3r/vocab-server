{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "DatabaseConfiguration": {
    "ConnectionString": "Server=localhost;Database=vocab;User Id=aspnet;Password=aspnet;Encrypt=false;"
  },

  "CorsConfiguration": {
    "Origins": [
      "http://localhost:3000"
    ]
  },

  "KeycloakConfiguration": {
    "MetadataAddress": "http://localhost:8080/auth/realms/vocab/.well-known/openid-configuration",
    "ValidIssuers": [ "http://localhost:8080/auth/realms/vocab"]
  }
}
